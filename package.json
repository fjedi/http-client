{
  "name": "@fjedi/http-client",
  "version": "1.4.0",
  "description": "Wrapper around axios to simplify its usage in fjedi projects",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "private": false,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "clean": "rimraf lib",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fjedi/http-client.git"
  },
  "keywords": [
    "fjedi",
    "http-client"
  ],
  "author": "Alexander Radyushin <r.alex.andr@ya.ru> (https://fjedi.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fjedi/http-client/issues"
  },
  "homepage": "https://github.com/fjedi/http-client#readme",
  "dependencies": {
    "@fjedi/errors": "1.1.2",
    "@fjedi/redis-client": "1.0.2",
    "axios": "0.21.1",
    "lodash": "4.17.21",
    "tunnel": "0.0.6"
  },
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.35",
    "@types/tunnel": "0.0.2",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  }
}
